<%- include("partials/header"); -%>


    <h1>Home</h1>
    <p> <%= startingContent %> </p>


    <% posts.forEach(function(post) { %>
        <h1> <%= post.title %></h1>
        <% const myString = post.content; %>
        <% var turncatedString = myString.substring(0,100); %>
        <p> <%= turncatedString + " .... " %> 
            <a href="/posts/<%= post.title %>">Read More</a>
        </p>
    <% }); %>


